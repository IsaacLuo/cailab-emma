{
  "compilerOptions": {
      "module": "commonjs",
      "lib": ["es6"],
      "target": "es2017",
      "noImplicitAny": false,
      "sourceMap": true,
      "outDir": "build",
      "rootDir": ".",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "jsx":"react",
  },
  "exclude": [
      "**/node_modules",
      "../node_modules",
      "**/*.spec.ts",
      "**/*.test.ts",
      "build"
  ],
  "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "**/*.json", "conf.ts"
, "src/renderManualProtocols.tsx"  ]
}